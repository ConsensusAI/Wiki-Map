        <br>
        <br>
        Edit map

      </section>

    <section class="form-container">
        <h1>Enter Point details</h1>
        Title:
        <br>
        <input type="pointTitle" name="pointTitle" placeholder="Name your point">
        <br><br>
        Description:
        <br>
        <input type="pointDescription" name="pointDescription" placeholder="What is this place?">
        <br><br>
        Image:
        <input type="pointImage" name="pointImage" placeholder="Image link">
        <br><br>
        Longitutude:
        <input type="pointLong" name="pointLong" placeholder="Point Longitude">
        <br><br>
        Latitude:
        <input type="pointLat" name="pointLat" placeholder="Point Latitude">
        <br><br>

      <button type="submit" class="btn btn-primary">Submit</button>
    </form>

      </section>


